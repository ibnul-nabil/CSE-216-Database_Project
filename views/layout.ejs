<!DOCTYPE html>
<html lang="en">
  <head>
    <title>
      Books
    </title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="container mt-4">
      
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>AUTHOR</th>
            <th>PUBLISHER</th>
            <th>PRICE</th>
            
          </tr>
        </thead>
        <tbody>
          <% if(data.length){ for(var i = 0; i< data.length; i++) {%>
          <tr>
            
            <td><%= data[i].BOOK_NAME%></td>
            <td><%= data[i].AUTHOR_NAME%></td>
            <td><%= data[i].PUBLISHER_NAME%></td>
            <td><%= data[i].PRICE%></td>
            
          </tr>
          <% } }else{ %>
          <tr>
            <td>No data ever existed.</td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>